	if (w.getBlock(x, y, z)==Blocks.air) {
		if ((w.getBlock(x, y-1, z)==Blocks.grass)) {
			int i=0;
			while () {
			w.setBlock(x, y, z, blocks.Cuprite_ore);	
		}}
		 
	}
	
		
	


}}
